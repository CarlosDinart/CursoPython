import random
n1 = int(input("Digite um numero ? "))
n2 = int(input("Digite outro numero ? "))
n3 = int(input("Digite mais um numero? "))
lista = [n1, n2, n3]
sorteado = random.choice(lista)
print("O numero sorteado foi", (sorteado))