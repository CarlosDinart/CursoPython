from math import sin, cos, tan, radians
a = float(input("Digite um angulo ? "))
seno = sin(radians(a))
cosseno = cos(radians(a))
tangente = tan(radians(a))
print("O angulo de {} !, \n Tem o seno de {:.2f}, tem o cosseno de {:.2f}, em a  tangente de {:.2f}".format(a, seno, cosseno, tangente))